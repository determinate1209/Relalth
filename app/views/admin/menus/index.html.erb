<h3>メニュー一覧</h3>
<% @menus.each do |menu| %>
  
  <%= link_to admin_menu_path(menu.id) do %>
    <%= menu.name %>
  <% end %>
  
  <%= menu.description %>
  
    <%= link_to "編集する", edit_admin_menu_path(menu.id) %>
    <%= link_to "削除する", admin_menu_path(menu.id), method: :delete %><br />
    
    
  
<% end %>


