<h3>メニュー作成</h3>
<%= form_with model: @menu, url: menus_path do |f| %>
 <%= f.label "メニュー名" %><br />
 <%= f.text_field :name %><br />
 
 <%= f.label "メニュー説明" %><br />
 <%= f.text_area :description %><br />
 
 <%= f.label :プラン %><br />
 <%= f.select :plan_name, Menu.plan_names.keys.map { |k| [t("enums.menu.plan_name.#{k}"), k] } %><br />
 
 <%= f.submit "メニューを作成" %>
 
<% end %>
